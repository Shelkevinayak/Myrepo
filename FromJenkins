pipeline {
   agent any
   
   environment {
       DockerProvisioningwithvolume='1.0'
   }

   stages {
      stage('stage-1') {
         steps {
            echo "This is build number $BUILD_NUMBER of demo $DockerProvisioningwithvolume"
            sh '''
               echo "Provisioning docker with volume now"
                chmod +x Docvol.sh
               ./Docvol.sh
            '''
         }
      }
   }
}
